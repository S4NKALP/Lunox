    <string name="tutorial_step_2_content">Tap any app name to open it. Apps are sorted alphabetically by default.</string>

    <string name="tutorial_step_3_title">Search</string>
    <string name="tutorial_step_3_content">Swipe down to up to open search. Start typing to find apps quickly.</string>

    <string name="tutorial_step_4_title">App Options</string>
    <string name="tutorial_step_4_content">Long press any app for options: rename, hide, colors, and more.</string>

    <string name="tutorial_step_5_title">Settings</string>
    <string name="tutorial_step_5_content">Long press empty space for launcher settings: themes, fonts, sorting.</string>

